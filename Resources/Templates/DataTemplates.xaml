<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:CashalotHelper.ViewModels"
                    xmlns:view="clr-namespace:CashalotHelper.Views">

    <DataTemplate DataType="{x:Type vm:BackupManagerViewModel}">
        <view:BackupManagerView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <view:SettingsView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>